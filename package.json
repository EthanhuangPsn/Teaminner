{
  "name": "game-voice-service",
  "version": "1.0.0",
  "description": "游戏语音服务 - 多人在线语音通信系统",
  "scripts": {
    "install:all": "cd backend && cnpm install && cd ../frontend && cnpm install",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "prisma:generate": "cd backend && npx prisma generate",
    "prisma:migrate": "cd backend && npx prisma migrate dev"
  },
  "keywords": [
    "webrtc",
    "voice",
    "game",
    "realtime"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.13.4",
    "socket.io-client": "^4.8.3"
  }
}
